{
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "env": {
      "PYTHONUNBUFFERED": "true",
      "APP_FILE": "backend.py",
      "APP_MODULE": "app",
      "PYTHON_VERSION": "3.8.0",
      "PIP_REQUIREMENTS": "requirements.txt"
    }
  }
  