{
  "builds": [
    {
      "src": "templates/index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/templates/index.html"
    }
  ],
    "env": {
      "PYTHONUNBUFFERED": "true",
      "APP_FILE": "backend.py",
      "APP_MODULE": "app",
      "PYTHON_VERSION": "3.8.0",
      "PIP_REQUIREMENTS": "requirements.txt"
    }
  }
  
